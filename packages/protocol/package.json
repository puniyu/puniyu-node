{
  "name": "@puniyu/protocol",
  "version": "0.2.4",
  "description": "puniyu的协议库",
  "type": "module",
  "types": "dist/index.d.ts",
  "main": "dist/index.js",
  "scripts": {
    "artifacts": "napi artifacts",
    "build": "napi build --platform --release -p puniyu_node_protocol --esm -o dist",
    "create-npm-dirs": "napi create-npm-dirs",
    "prepublishOnly": "napi prepublish --no-gh-release"
  },
  "files": [
    "dist"
  ],
  "homepage": "https://github.com/puniyu/puniyu-node",
  "bugs": {
    "url": "https://github.com/puniyu/puniyu-node/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/puniyu/puniyu-node.git"
  },
  "napi": {
    "binaryName": "protocol",
    "targets": [
      "x86_64-pc-windows-msvc",
      "x86_64-apple-darwin",
      "x86_64-unknown-linux-gnu",
      "aarch64-unknown-linux-gnu",
      "aarch64-apple-darwin"
    ],
    "dtsHeader": "// powered by puniyu\n"
  },
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "keywords": [
    "puniyu"
  ],
  "author": "puniyu",
  "license": "LGPL-3.0-only"
}